@model Sampoerna.EMS.Website.Models.PBCK7AndPBCK3.Pbck7Pbck3CreateViewModel
@using Sampoerna.EMS.Website.Helpers

<div id="printout" class="tab-pane" role="tabpanel">
    <div aria-multiselectable="true" role="tablist" id="accordionPrint" class="panel-group">
        <div class="panel panel-default">
            <div id="headingPrint" role="tab" class="panel-heading">
                <h4 class="panel-title">
                    <a aria-controls="collapsePrint" aria-expanded="true" href="#collapsePrint" data-parent="#accordionPrint" data-toggle="collapse">
                        Print Out
                        <i class="fa fa-caret-down"></i>
                    </a>
                </h4>
            </div>
            <div aria-labelledby="headingPrint" role="tabpanel" class="panel-collapse collapse in" id="collapsePrint">
                <div class="panel-body">
                    <div class="row">
                        <div role="form" class="form-excise ">
                            <div class="last-form">
                                <div class="col-md-3">
                                    <div class="row">
                                        <label class="col-md-5">PBCK-3 No</label>
                                        <div class="col-md-7">
                                            <input type="text" placeholder="" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <label class="col-md-3">Date</label>
                                        <div class="col-md-9">
                                            <input type="text" placeholder="" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <label class="col-md-3">User</label>
                                        <div class="col-md-9">
                                            <input type="text" placeholder="" class="form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <button class="btn btn-blue full">Print</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="table-container" id="PrintOutHistoryTable">
        @Html.Partial("_PrintHistoryTable")
    </div>
</div>

