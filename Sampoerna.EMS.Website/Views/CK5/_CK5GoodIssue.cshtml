@model Sampoerna.EMS.Website.Models.CK5.CK5FormViewModel

<div class="row">
    <div class="col-md-6">
        <div class="row">
            <label class="col-md-4">GI Date<span class="red">*</span></label>
            <div class="col-md-8">
                <div class="input-group date">
                    @Html.TextBoxFor(model => model.GiDate, "{0:dd MMM yyyy}", new { @class = "form-control" })
                    <span class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </span>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="row">
           
        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="row">
            <label class="col-md-4">Sealing Notification Number<span class="red">*</span></label>
            <div class="col-md-8">
                @Html.TextBoxFor(model => model.SealingNotifNumber, new { @class = "form-control" })
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="row">
            <label class="col-md-4">Sealing Notification Date<span class="red">*</span></label>
            
            <div class="col-md-8">
                <div class="input-group date">
                    @Html.TextBoxFor(model => model.SealingNotifDate, "{0:dd MMM yyyy}", new { @class = "form-control" })
                    <span class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </span>
                </div>
            </div>

        </div>
    </div>
</div>

