@model Sampoerna.EMS.Website.Models.CK5.CK5FormViewModel

<div role="tabpanel" class="tab-pane" id="upload">
    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingOne">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        Upload Excel File
                    </a>
                </h4>
            </div>
            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="input-group">
                                <input type="file" class="file-inputs" data-filename-placement="inside" name="itemExcelFile" id="itemExcelFile">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <input type="button" id="CK5UploadSubmitBtn" class="btn btn-grey generate" value="Generate" disabled />
                            <input type="button" class="btn btn-grey" id="CK5UploadSave" value="Save" disabled />
                            
                            <input type="button" class="btn btn-blue" id="btnAddRow" value="AddRow"  />
                            @*<a id="btnAddNew" data-modal="" data-oncomplete=""  href="InputMaterials" class="btn btn-primary">Add</a>*@
                        </div>

                    </div>
                </div>
            </div>
        </div>



    </div>

    <div class="table-container" id="ProdConvContent">
        @Html.Partial("_CK5UploadList", Model.UploadItemModels)
    </div>

</div>



