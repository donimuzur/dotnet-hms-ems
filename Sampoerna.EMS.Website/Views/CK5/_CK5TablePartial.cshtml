@model Sampoerna.EMS.Website.Models.CK5.CK5IndexViewModel


<table id="ck5Table" class="table cell-border display no-wrap" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Action</th>
            <th>No</th>
            <th>Document Number</th>
            <th>Qty - Type of Packaging</th>
            <th>POA</th>
            <th>NPPBKC ID</th>
            <th>Source Plant</th>
            <th>Destination Plant</th>
            <th>Status</th>
        </tr>
    </thead>
    <thead class="filters">
        <tr>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @{
            int rownum = 1;
        }
        @foreach (var item in Model.DetailsList)
        {
            <tr>
                <td class="action">
                    <a href="#"><i class="fa fa-eye"></i></a>
                    <a href="#"><i class="fa fa-pencil-square-o"></i></a>
                </td>
                <td>@rownum</td>
                <td>@Html.DisplayFor(model => item.DocumentNumber)</td>
                <td>@Html.DisplayFor(model => item.Qty)</td>
                <td>
                    @Html.DisplayFor(model => item.POA)
                </td>
                <td>
                    @Html.DisplayFor(model => item.NPPBKC_ID)
                </td>
                <td>@Html.DisplayFor(model => item.SourcePlant)</td>
                <td>@Html.DisplayFor(model => item.DestinationPlant)</td>
                <td class="last">@Html.DisplayFor(model => item.Status)</td>
            </tr>
            rownum++;
        }
    </tbody>
</table>

