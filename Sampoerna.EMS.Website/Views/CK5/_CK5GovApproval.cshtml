@Html.HiddenFor(model => model.SubmissionDate)
@Html.HiddenFor(model => model.IsWaitingGovApproval)

<div role="tabpanel" class="tab-pane active" id="information">
    <div class="tab-wrapper">


        <div class="panel-group" id="accordion-2" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="headingTwo">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion-2" href="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                            Origin Plant
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </h4>
                </div>
                <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                    <div class="panel-body">
                        <div class="form-excise" role="form">
                            @if (Model.IsCk5PortToImporter)
