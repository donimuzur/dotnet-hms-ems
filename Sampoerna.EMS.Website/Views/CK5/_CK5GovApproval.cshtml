                        </a>
                    </h4>
                </div>
                <div id="collapse6" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading6">
                    <div class="panel-body">
                        <div class="form-excise" role="form">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Invoice Number</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.InvoiceNumber, new { @disabled = false, @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Invoice Date</label>
                                        <div class="col-md-8">

                                            @Html.TextBoxFor(model => model.InvoiceDate, new { @disabled = false, @class = "form-control" })

                                        </div>
                                    </div>
                                </div>
                            </div>
                            @if (!Model.IsCk5Export && !Model.IsCk5PortToImporter)
                            {
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4 long">PBCK-1 Decree  Number</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.PbckDecreeNumber, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">PBCK-1 Decree Date</label>
                                            <div class="col-md-8">
                                                @Html.EditorFor(model => model.PbckDecreeDate)
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
