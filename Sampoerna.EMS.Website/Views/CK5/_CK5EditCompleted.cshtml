@using Sampoerna.EMS.Core
@using Sampoerna.EMS.Website.Helpers
@model Sampoerna.EMS.Website.Models.CK5.CK5FormViewModel

<div role="tabpanel" class="tab-pane active" id="information">
    <div class="tab-wrapper">


        <div class="panel-group" id="accordion-2" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="headingTwo">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion-2" href="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                            Origin Plant
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </h4>
                </div>
                <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                    <div class="panel-body">
                        <div class="form-excise" role="form">

                            <div class="row">

                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Plant</label>
                                        <div class="col-md-8">
                                            @if (Model.IsCk5PortToImporter && Model.IsDomesticAlcohol)
                                            {
                                                @Html.TextBoxFor(model => model.SourcePlantName, new { @disabled = false, @class = "form-control" })
                                            }
                                            else
                                            {
                                                @Html.TextBoxFor(model => model.SourcePlantId, new { @disabled = false, @class = "form-control" })
                                            }
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">NPWP</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.SourceNpwp, new { @disabled = "disabled", @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">

                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">NPPBKC ID</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.SourceNppbkcId, new { @disabled = "disabled", @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Company Name</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.SourceCompanyName, new { @disabled = "disabled", @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Address</label>
                                        <div class="col-md-8">
                                            @Html.TextAreaFor(model => model.SourceAddress, new { @row = "5", @disabled = "disabled", @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">KPPBC Name, Office ID</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.SourceKppbcName, new { @disabled = "disabled", @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @if (Model.IsCk5Manual && Model.Ck5ManualType == Enums.Ck5ManualType.Trial)
                            {
                                <div class="row" id="idFlagNppbkc">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">NPPBKC Import</label>
                                            <div class="col-md-8">
                                                @Html.CheckBoxFor(model => model.FLAG_NPPBKC_IMPORT, new { @disabled = "disabled" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="panel-group" id="accordion-3" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="headingThree">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="true" aria-controls="collapseThree">
                            Destination Plant
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </h4>
                </div>
                <div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                    <div class="panel-body">
                        <div class="form-excise" role="form">

                            @if (Model.IsCk5Export)
                            {
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Destination Country</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DisplayDetailsDestinationCountry, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Company Name</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DestCompanyName, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Address</label>
                                            <div class="col-md-8">
                                                @Html.TextAreaFor(model => model.DestAddress, new { @disabled = "disabled", @class = "form-control", @row = "5" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Loading Port</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.LoadingPort, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Loading Port Name & ID</label>
                                            <div class="col-md-8">
                                                <div class="row">
                                                    <div class="col-md-8">
                                                        @Html.TextBoxFor(model => model.LoadingPortName, new { @disabled = "disabled", @class = "form-control" })
                                                    </div>
                                                    <div class="col-md-4">
                                                        @Html.TextBoxFor(model => model.LoadingPortId, new { @disabled = "disabled", @class = "form-control" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">

                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Final Port</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.FinalPort, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Final Port Name & ID</label>
                                            <div class="col-md-8">
                                                <div class="row">
                                                    <div class="col-md-8">
                                                        @Html.TextBoxFor(model => model.FinalPortName, new { @disabled = "disabled", @class = "form-control" })
                                                    </div>
                                                    <div class="col-md-4">
                                                        @Html.TextBoxFor(model => model.FinalPortId, new { @disabled = "disabled", @class = "form-control" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            }
                            else
                            {
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Plant</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DestPlantId, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">NPWP</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DestNpwp, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">NPPBKC ID</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DestNppbkcId, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Company Name</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DestCompanyName, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Address</label>
                                            <div class="col-md-8">
                                                @Html.TextAreaFor(model => model.DestAddress, new { @disabled = "disabled", @class = "form-control", @row = "5" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">KPPBC Name, Office ID</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.DestKppbcName, new { @disabled = "disabled", @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="headingOne">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            CK-5 Detail
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </h4>
                </div>
                <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                    <div class="panel-body">
                        <div class="form-excise" role="form">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">CK-5 Type</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.Ck5Type, new { @disabled = false, @class = "form-control" })

                                        </div>
                                    </div>
                                </div>
                                @if (Model.IsCk5ImporterToPlant)
                                {
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Port to Importer Ref</label>
                                            <div class="col-md-8">
                                                @Html.DropDownListFor(model => model.Ck5RefId, Model.Ck5RefList, "Select", new { @class = "form-control", @disabled = "disabled" })

                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">KPPBC City</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.KppBcCity, new { @disabled = false, @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">@Html.LabelFor(model => model.CeOfficeCode)</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.CeOfficeCode, new { @disabled = false, @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Submission Number</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.SubmissionNumber, new { @class = "form-control", @disabled = "disabled" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Submission Date</label>
                                        <div class="col-md-8">
                                            @Html.EditorFor(model => model.SubmissionDate)

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Registration Number</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.RegistrationNumber, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Registration Date</label>
                                        <div class="col-md-8">
                                            <div class="input-group date">
                                                @Html.TextBoxFor(model => model.RegistrationDate, "{0:dd MMM yyyy}", new { @class = "form-control" })
                                                <span class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Goods Type Group</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.GoodTypeName, new { @disabled = false, @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Excise Settlement</label>
                                        <div class="col-md-8">
                                            @Html.EnumDropDownListFor(model => model.ExciseSettlement, Model.ExciseSettlementList, new { @class = "form-control" })

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Excise Status</label>
                                        <div class="col-md-8">
                                            @Html.EnumDropDownListFor(model => model.ExciseStatus, Model.ExciseStatusList, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Request Type</label>
                                        <div class="col-md-8">
                                            @*@Html.EnumDropDownListFor(model => model.RequestType, Model.RequestTypeList, new { @class = "form-control" })*@
                                            @Html.EnumDropDownListFor(model => model.RequestType, Model.RequestTypeList, "Select", new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @if (Model.IsCk5Manual)
                            {
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">CK-5 Manual Type</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.Ck5ManualTypeString, new { @class = "form-control", @disabled = false })

                                            </div>
                                        </div>
                                    </div>
                                    @if (Model.Ck5ManualType == Enums.Ck5ManualType.Trial)
                                    {
                                        <div class="col-md-3">
                                            <div class="row" id="ck5TrialPbck1Reduce">
                                                <label class="col-md-4">Reduce PBCK-1</label>
                                                <div class="col-md-8">
                                                    @Html.CheckBoxFor(model => model.IsReducePbck1Ck5Trial, new { @class = "checkbox", @disabled = "disabled" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="row" id="ck5ManualLab" style="display: none">
                                                <label class="col-md-4">LACK-1 Lab</label>
                                                <div class="col-md-8">
                                                    @Html.CheckBoxFor(model => model.IsLack1Lab, new { @class = "checkbox", @disabled = "disabled" })
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>


                            }

                        </div>

                    </div>
                </div>

            </div>
        </div>

        <div class="panel-group" id="accordion-6" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="heading6">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion-6" href="#collapse6" aria-expanded="true" aria-controls="collapse6">
                            Notification Data
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </h4>
                </div>
                <div id="collapse6" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading6">
                    <div class="panel-body">
                        <div class="form-excise" role="form">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Invoice Number</label>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.InvoiceNumber, new { @class = "form-control", @maxlength = "50" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Invoice Date</label>
                                        <div class="col-md-8">
                                            <div class="input-group date">
                                                @Html.TextBoxFor(model => model.InvoiceDate, "{0:dd MMM yyyy}", new { @class = "form-control" })
                                                <span class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @if (!Model.IsCk5Export && !Model.IsCk5PortToImporter && !Model.IsTriggerSto && !Model.IsCk5Waste)
                            {
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4 long">PBCK-1 Decree  Number</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.PbckDecreeNumber, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">PBCK-1 Decree Date</label>
                                            <div class="col-md-8">
                                                @Html.EditorFor(model => model.PbckDecreeDate)
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Pbck1 Qty Approved</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.Pbck1QtyApproved, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">CK5 Total Qty</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.Ck5TotalExciseable, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <label class="col-md-4">Remain</label>
                                            <div class="col-md-8">
                                                @Html.TextBoxFor(model => model.RemainQuota, new { @disabled = false, @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Carriage Method</label>
                                        <div class="col-md-8">
                                            @Html.EnumDropDownListFor(model => model.CarriageMethod, Model.CarriageMethodList, "Select", new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <label class="col-md-4">Grand Total Exciseable</label>
                                        <div class="col-md-4">
                                            @Html.TextBoxFor(model => model.GrandTotalExDisplay, new { @disabled = false, @class = "form-control" })
                                        </div>
                                        <div class="col-md-4">
                                            @Html.TextBoxFor(model => model.PackageUomName, new { @disabled = false, @class = "form-control" })
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <div class="panel-group" id="accordion-5" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="heading5">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion-5" href="#collapse5" aria-expanded="true" aria-controls="collapse5">
                            Upload
                            <i class="fa fa-caret-down"></i>
                        </a>
                    </h4>
                </div>
                <div id="collapse5" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading5">
                    <div class="panel-body">

                        <div class="table-container" id="ck5DetailMaterialTable">

                            <table id="Ck5UploadTable" class="table table-bordered table-striped js-options-table">
                                <thead>
                                    <tr>

                                        <th>Material Number</th>
                                        <th>Qty</th>
                                        <th>UOM</th>
                                        <th>Convertion</th>
                                        <th>Converted Qty</th>
                                        <th>Converted UOM</th>
                                        <th>HJE</th>
                                        <th>Tariff</th>
                                        <th>Excise Value</th>
                                        <th>USD Value</th>
                                        <th>Note</th>
                                        @if (Model.IsCk5Waste)
                                        {
                                            <th>Waste Stock</th>
                                        }
                                        else
                                        {
                                            <th>Message</th>
                                        }

                                    </tr>
                                </thead>
                                <tbody>


                                    @if (Model != null)
                                    {
                                        for (int i = 0; i < Model.UploadItemModels.Count(); i++)
                                        {
                                             @Html.HiddenFor(model => model.UploadItemModels[i].CK5_MATERIAL_ID)
                                            <tr>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].Brand)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].Qty)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].Uom)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].Convertion)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].ConvertedQty)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].ConvertedUom)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].Hje)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].Tariff)</td>
                                                <td>@Html.DisplayFor(model => model.UploadItemModels[i].ExciseValue)</td>
