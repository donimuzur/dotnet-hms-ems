@model Sampoerna.EMS.Website.Models.CK5.CK5MarketReturnSummaryReportsViewModel

<table id="ck5Table" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th class="toggle-row"> No </th>
            <th>CK-5 Number</th>
            <th>Plant ID</th>
            <th>Plant Desc</th>
            <th>NPPBKC</th>
            <th>KPPBC</th>
            <th>Date</th>
            <th>Request Type</th>
            <th>Exec Date From</th>
            <th>Exec Date To</th>
            <th>FA Code</th>
            <th>Brand</th>
            <th>Content</th>
            <th>HJE</th>
            <th>Tariff</th>
            <th>CK-5 Market Return Pack Qty</th>
            <th>Fiscal Year</th>
            <th>Excise Value</th>
            <th>BACK-1 No</th>
            <th>BACK-1 Date</th>
            <th>PBCK-3 No.</th>
            <th>PBCK-3 Status</th>
            <th>CK-2 No.</th>
            <th>CK-2 Value</th>
            <th>Completed Date</th>
            <th>POA Approved by</th>
            <th>Creator</th>
            <th>Status</th>
        </tr>
    </thead>

    <tbody>
        @{
            int row = 1;

            foreach (var item in Model.DetailsList)
            {
                <tr>
                    <td>@row</td>
                    <td>@Html.DisplayFor(model => item.Ck5Number)</td>
                    <td>@Html.DisplayFor(model => item.PlantId)</td>
                    <td>@Html.DisplayFor(model => item.PlantDesc)</td>
                    <td>@Html.DisplayFor(model => item.Nppbkc)</td>
                    <td>@Html.DisplayFor(model => item.Kppbc)</td>
                    <td>@Html.DisplayFor(model => item.Date)</td>
                    <td>@Html.DisplayFor(model => item.ReqType)</td>
                    <td>@Html.DisplayFor(model => item.ExecDateFrom)</td>
                    <td>@Html.DisplayFor(model => item.ExecDateTo)</td>
                    <td>@Html.DisplayFor(model => item.FaCode)</td>
                    <td>@Html.DisplayFor(model => item.Brand)</td>
                    <td>@Html.DisplayFor(model => item.Content)</td>
                    <td>@Html.DisplayFor(model => item.Hje)</td>
                    <td>@Html.DisplayFor(model => item.Tariff)</td>
                    <td>@Html.DisplayFor(model => item.Ck5MarketReturnQty)</td>
                    <td>@Html.DisplayFor(model => item.FiscalYear)</td>
                    <td>@Html.DisplayFor(model => item.ExciseValue)</td>
                    <td>@Html.DisplayFor(model => item.Back1No)</td>
                    <td>@Html.DisplayFor(model => item.Back1Date)</td>
                    <td>@Html.DisplayFor(model => item.Pbck3No)</td>
                    <td>@Html.DisplayFor(model => item.Pbck3Status)</td>
                    <td>@Html.DisplayFor(model => item.Ck2Number)</td>
                    <td>@Html.DisplayFor(model => item.Ck2Value)</td>
                    <td>@Html.DisplayFor(model => item.CompletedDate)</td>
                    <td>@Html.DisplayFor(model => item.Poa)</td>
                    <td>@Html.DisplayFor(model => item.Creator)</td>
                    <td>@Html.DisplayFor(model => item.Status)</td>
                </tr>
                row = row + 1;
            }
        }
    </tbody>
</table>

