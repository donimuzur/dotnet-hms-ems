@model Sampoerna.EMS.Website.Models.CK5.CK5FormViewModel

<div class="row">
    <div class="col-md-6">
        <div class="row">
            @Html.LabelFor(model => model.GiDate, new { @class = "col-md-4" })
            <div class="col-md-8">
                @Html.EditorFor(model => model.GiDate)
            </div>
        </div>
    </div>
   
</div>

<div class="row">
    <div class="col-md-6">
        <div class="row">
            @Html.LabelFor(model => model.SealingNotifNumber, new { @class = "col-md-4" })
            <div class="col-md-8">
                @Html.TextBoxFor(model => model.SealingNotifNumber, new { @class = "form-control", @disabled = "true" })
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="row">
            @Html.LabelFor(model => model.SealingNotifDate, new { @class = "col-md-4" })
            <div class="col-md-8">
                @Html.EditorFor(model => model.SealingNotifDate)
            </div>

        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="row">
            @Html.LabelFor(model => model.GrDate, new { @class = "col-md-4" })
            <div class="col-md-8">
                <div class="input-group date">
                    @Html.TextBoxFor(model => model.GrDate, "{0:dd MMM yyyy}", new { @class = "form-control" })
                    <span class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </span>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="row">

        </div>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="row">
            @Html.LabelFor(model => model.UnSealingNotifNumber, new { @class = "col-md-4" })
            <div class="col-md-8">
                @Html.TextBoxFor(model => model.UnSealingNotifNumber, new { @class = "form-control" })
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="row">
            @Html.LabelFor(model => model.UnsealingNotifDate, new { @class = "col-md-4" })
            <div class="col-md-8">
                <div class="input-group date">
                    @Html.TextBoxFor(model => model.UnsealingNotifDate, "{0:dd MMM yyyy}", new { @class = "form-control" })
                    <span class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </span>
                </div>
            </div>

        </div>
    </div>
</div>
