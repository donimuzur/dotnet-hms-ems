@model Sampoerna.EMS.Website.Models.XmlLog.XmlLogIndexViewModel

<table id="xmlLogTable" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th class="toggle-row"> Filter </th>
            <th>Date Time</th>
            <th>Level</th>
            <th>Type</th>
            <th>Logger</th>
            <th>Message</th>
            <th>File Name</th>
        </tr>
    </thead>
    <thead class="filters">
        <tr>
            <th> Action</th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.ListXmlLogs)
        {
            <tr>
                <td class="action">
                    <a href="@Url.Action("Detail", "XmlLog", new { id = item.XmlLogId })" data-toggle="tooltip" data-placement="top" title="Details"><i class="fa fa-eye"></i></a>
                </td>
                <td>@Html.DisplayFor(model => item.TimeStampDisplay)</td>
                <td>@Html.DisplayFor(model => item.Level)</td>
                <td>@Html.DisplayFor(model => item.Type)</td>
                <td>@Html.DisplayFor(model => item.Logger)</td>
                <td>@Html.DisplayFor(model => item.Message)</td>
                <td>@Html.DisplayFor(model => item.FileName)</td>
            </tr>

        }
    </tbody>
</table>

