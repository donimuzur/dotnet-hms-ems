
@model Sampoerna.EMS.Website.Models.BrandRegistrationTransaction.ProductDevelopment.ProductDevelopmentViewModel


<div id="modalApprove" class="modal ems-modal fade approve-modal product" tabindex="-1" role="dialog" aria-labelledby="ApproveModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                <h4 class="modal-title">Request for Approve</h4>
            </div>
            <div class="modal-body">

                <div class="row col-md-12">
                    <label>You are going to approve this items. Are you sure?</label>
                </div>

            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-blue btn-sm" id="ApproveButtonSubmit">Approve</button>
                <button type="button" class="btn btn-blue btn-sm" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


