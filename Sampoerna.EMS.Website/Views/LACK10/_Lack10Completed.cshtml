@model Sampoerna.EMS.Website.Models.LACK10.Lack10IndexViewModel

<table id="ListLack10" class="table table-striped table-hover" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th class="toggle-row"> Action </th>
            <th> LACK-10 No </th>
            <th> Company </th>
            <th> Based On </th>
            <th> NPPBKC </th>
            <th> Plant Id </th>
            <th> Month </th>
            <th> Year </th>
            <th> Status </th>
        </tr>

    </thead>
    <thead class="filters">
        <tr>
            <th> Filter </th>
            <th> </th>
            <th> </th>
            <th> </th>
            <th> </th>
            <th> </th>
            <th> </th>
            <th> </th>
            <th> </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.Detail)
        {
            <tr>
                <td class="action">
                    <a href="@Url.Action("Detail", "LACK10", new { id = item.Lack10Id })" data-tooggle="tooltip" data-placement="top" title="Detail"><i class=" fa fa-eye"></i></a>
                    @if (Model.IsNotViewer)
                    {
                        <a href="@Url.Action("Details", "LACK10", new { id = item.Lack10Id })" data-tooggle="tooltip" data-placement="top" title="Edit"><i class=" fa fa-pencil-square-o"></i></a>
                    }
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Lack10Number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CompanyName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BasedOn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NppbkcId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PlantId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ReportedMonthName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PeriodYears)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StatusName)
                </td>

            </tr>
        }
    </tbody>
</table>