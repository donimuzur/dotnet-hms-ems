@model Sampoerna.EMS.Website.Models.NPPBKC.NPPBKCViewModels
@{
    ViewBag.Title = "NPPBKC";
}

@using (Html.BeginForm("Edit", "NPPBKC", null, FormMethod.Post, new { @class = "form-excise", role = "form-control" }))
{
    <div class="container-wrap title-page">
        <div class="row">
            <div class="col-sm-12">
                <h3>Update POA</h3>
                <div class="action-button">
                    <input type="submit" class="btn btn-blue" value="Save" />
                    <a href="@Url.Action("Index", "NPPBKC")" class="btn btn-grey">Cancel</a>
                </div>
            </div>
        </div>
    </div>

    <div class="container-wrap">
        <div class="row">
            <div class="col-sm-12">
                <div class="white-wrapper">
                    <div class="scroll">
                        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="headingOne">
                                    <h4 class="panel-title">
                                        <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                            Update NPPBKC
                                            <i class="fa fa-caret-down"></i>
                                        </a>
                                    </h4>
                                </div>
                                <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                    <div class="panel-body">
                                        <form class="form-excise" role="form">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">NPPBKC ID</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(model => model.Detail.NppbckNo, new {@class ="form-control"} )
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">City</label>
                                                        <div class="col-md-9">
                                                            <input type="text" class="form-control" placeholder="" disabled>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">Address 1</label>
                                                        <div class="col-md-9">
                                                            <textarea class="form-control" rows="5" id="comment" disabled></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3 long">Region Office of DGCE</label>
                                                        <div class="col-md-9">
                                                            <textarea class="form-control" rows="5" id="comment"></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">Address 2</label>
                                                        <div class="col-md-9">
                                                            <textarea class="form-control" rows="5" id="comment" disabled></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">Text to</label>
                                                        <div class="col-md-9">
                                                            <textarea class="form-control" rows="5" id="comment"></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">KPPBC ID</label>
                                                        <div class="col-md-9">
                                                            <input type="text" class="form-control" placeholder="" disabled>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">City Alias</label>
                                                        <div class="col-md-9">
                                                            <input type="text" class="form-control" placeholder="">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3">Region</label>
                                                        <div class="col-md-9">
                                                            <input type="text" class="form-control" placeholder="" disabled>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3 long">Account Number</label>
                                                        <div class="col-md-9">
                                                            <input type="text" class="form-control" placeholder="" disabled>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <label class="col-md-3 long">Start Date</label>
                                                        <div class="col-md-9">
                                                            <div class="row">
                                                                <div class="col-md-6">
                                                                    <input type="text" class="form-control" placeholder="" disabled>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <input type="text" class="form-control" placeholder="" disabled>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

}