@model Sampoerna.EMS.Website.Models.LACK2.LACK2CreateViewModel
<div role="tabpanel" class="tab-pane" id="printout">
    <div class="panel-group" id="accordionPrint" role="tablist" aria-multiselectable="true">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingPrint">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordionPrint" href="#collapsePrint" aria-expanded="true" aria-controls="collapsePrint">
                        Print Out
                        <i class="fa fa-caret-down"></i>
                    </a>
                </h4>
            </div>
            <div id="collapsePrint" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingPrint">
                <div class="panel-body">
                    <div class="row">
                        <form class="form-excise " role="form">
                            <div class="last-form">
                                <div class="col-md-3">
                                    <div class="row">
                                        <label class="col-md-5">LACK-2 No</label>
                                        <div class="col-md-7">
                                            <input type="text" class="form-control" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <label class="col-md-3">Date</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <label class="col-md-3">User</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <button class="btn btn-blue full">Print</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="table-container">
        <table class="table table-bordered table-striped js-options-table">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>User</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td> </td>
