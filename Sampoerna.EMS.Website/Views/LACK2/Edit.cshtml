    function GetCk5ByPeriod() {
        var month = parseInt($('#Lack2Model_PeriodMonth').val());
        var year = parseInt($('#Lack2Model_PeriodYear').val());
        var goodstype = $('#Lack2Model_ExGoodTyp').find("option:selected").val();
        var sendPlantId = $('#Lack2Model_LevelPlantId').find("option:selected").val();
        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetCK5ByLack2Period", "LACK2")',
            data: { month: month, year: year, sendPlantId: sendPlantId, goodstype: goodstype },
            success: function (data) {

                if (data.length > 0) {
                    $('#tb-body-ck5-lack2').html("");
                    for (var i = 0; i < data.length; i++) {
                        var tableCk5Item = '<tr><td><input type="hidden" id="Lack2Model_Items[' + i + ']__Ck5Id" name="Lack2Model.Items[' + i + '].Ck5Id" value=' + data[i].CK5_ID + '></input>' + (parseInt(i) + parseInt(1)) + '</td>' +
                            '<td>' + data[i].SUBMISSION_NUMBER + '</td>' +
                            '<td>' + data[i].GIDateStr + '</td>' +
                            '<td>' + data[i].GRAND_TOTAL_EX + '</td>' +
                            '<td>' + data[i].DEST_PLANT_COMPANY_NAME + '</td>' +
                            '<td>' + data[i].DEST_PLANT_NPPBKC_ID + '</td>' +
                            '<td>' + data[i].DEST_PLANT_ADDRESS + '</td></tr>';
                        $('#tb-body-ck5-lack2').append(tableCk5Item);
                    }
                }
            }
        });
        

    }
